  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^8.2.5",
    "concurrently": "^8.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "install": "^0.13.0",
    "nodemon": "^3.0.3",
    "npm": "^10.2.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
