{
  "name": "blog-code-references",
  "version": "1.0.0",
  "description": "This branch contains code mentioned in the article [HERE](https://dev.to/yvad60/how-to-create-a-react-app-without-using-create-react-app-a-step-by-step-guide-30nl)",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server .",
    "build": "webpack .",
    "clean": "rm -rf ./dist",
    "prod:build": "npm run clean && webpack --config webpack.config.js",
    "prod:server": "npm run prod:build && node ./dist/server.cjs",
    "local:build:server": "webpack --config webpack.server.config.js --watch",
    "local:start:server": "nodemon --watch ./dist/server.cjs --exec node ./dist/server.cjs",
    "local:web": "webpack --config webpack.client.config.js --watch",
    "local:server": "concurrently \"npm run local:build:server\" \"npm run local:start:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yvad60/blog-code-references.git"
  },
